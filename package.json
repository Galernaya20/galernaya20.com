{
  "name": "galernaya-20-site",
  "description": "Galernaya 20 site",
  "version": "0.0.1",
  "author": "SPB-Frontend <hi@spb-frontend.ru>",
  "dependencies": {
    "classnames": "^2.2.5",
    "next": "^4.1.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "video-react": "^0.8.9"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier-eslint --trailing-comma all --no-semi --single-quote --print-width 120 --jsx-bracket-same-line --write 'package.json' 'src/**/*.js' 'stories/**/*.js'",
    "test": "npm run lint",
    "lint": "eslint ./src ./stories & stylelint \"./src/{components,css}/*.css\"",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "babel-eslint": "^8.0.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "prettier-eslint-cli": "^4.4.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.8.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "format",
      "eslint",
      "git add"
    ],
    "*.{css}": [
      "stylelint"
    ]
  }
}
